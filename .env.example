# Database settings
POSTGRES_USER=n8n_prod
POSTGRES_PASSWORD=your_strong_password_here
POSTGRES_DB=n8n_prod

# n8n settings
N8N_ENCRYPTION_KEY=32_char_encryption_key_here
N8N_USER_MANAGEMENT_JWT_SECRET=32_char_jwt_secret_here

# Production settings
N8N_PROTOCOL=https
N8N_HOST=your.domain.com
N8N_PORT=5678
NODE_ENV=production

# Security settings
N8N_SECURE_COOKIE=true
N8N_AUTH_EXCLUDE_ENDPOINTS=
N8N_BASIC_AUTH_ACTIVE=false
N8N_JWT_AUTH_ACTIVE=false
N8N_USER_MANAGEMENT_DISABLED=false
N8N_DISABLE_PRODUCTION_MAIN_PROCESS=false
N8N_SKIP_WEBHOOK_AUTHENTICATION=false
N8N_SECURITY_OAUTH2_LOGIN_ENABLED=false
N8N_USER_MANAGEMENT_LOGIN_REQUIRED=true
N8N_USER_MANAGEMENT_INVITE_URL_ALLOWED=true

# Email settings (optional)
N8N_EMAIL_MODE=smtp
N8N_SMTP_HOST=smtp.example.com
N8N_SMTP_PORT=587
N8N_SMTP_USER=your_smtp_user
N8N_SMTP_PASS=your_smtp_password
N8N_SMTP_SENDER=n8n@your.domain.com
N8N_SMTP_SSL=false
N8N_SMTP_TLS=true
N8N_SMTP_AUTH=true

# Webhook URL (optional)
N8N_WEBHOOK_URL=https://your.domain.com/ 